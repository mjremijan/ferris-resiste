<#ftl output_format="plainText">
<html>
    <body>
        <!-- contents -->
        <div style="border: 0px solid black;">${contents}</div>

        <!-- apa reference -->
        <div style="clear:both; border: 0px solid black; font-family:Georgia,serif; margin-top:20px;">
            <p>
                ${author}.&nbsp;&nbsp;
                (${publishedDate}).&nbsp;&nbsp;
                <span style="font-style: italic;">${title}</span>.&nbsp;&nbsp;
                Retrieved from <a href="${linkHref}">${linkText}</a>.
            </p>
        </div>

        <!-- version -->
        <div style="border: 0px solid black;">
			<div style="float:left; padding-right:8px;">
				<p>
					<img style="" alt="Resiste icon" src="data:image/png;base64," />
				</p>
			</div>
			<div style="float: left; font-family: 'Franklin Gothic Book',sans-serif;">
				<p>
					${projectFinalName}
					<br />
					<a href="${projectUrl}">${projectUrl}</a>
				</p>
			</div>
			<div style="clear: both;"></div>
			<!-- end version -->
		</div>
    </body>
</html>
<html>
    <body>
        <!-- contents -->
        <p style="font-size: larger; font-weight: bold;">
           Contents
        </p>
        <div style="border:0px solid black; font-family:Georgia,serif;">${contents}</div>
        
        
        <!-- images -->
       <#if images?size != 0>
           <p style="font-size: larger; font-weight: bold;">
               Images
           </p>
             <#list images as image>
               <div style="border: 0px solid blue; white-space:nowrap; max-width:99% ">
                   <img style="max-width:99%;" src="${image.url}" />
               </div>
             </#list>
       </#if>        
                        
        <!-- media files -->
        <#if mediaFiles?size != 0>
            <p style="font-size: larger; font-weight: bold;">
                Media Files
            </p>
            <#list mediaFiles as mediaFile>
                <p>
                    ${mediaFile.type}: <a href="${mediaFile.url}">${mediaFile.url}</a>
                </p>
            </#list>
        </#if>

        <!-- apa reference -->
        <div style="clear:both; border: 0px solid black; font-family:Georgia,serif; margin-top:20px;">
            <p>
                ${author}.&nbsp;&nbsp;
                (${publishedDate}).&nbsp;&nbsp;
                <span style="font-style: italic;">${title}</span>.&nbsp;&nbsp;
                Retrieved from <a href="${linkHref}">${linkText}</a>.&nbsp;&nbsp;GUID ${guid}
            </p>
        </div>

        <!-- version -->
        <div style="border: 0px solid blue; white-space:nowrap; max-width:99% ">
            <div style="border:0px solid black; padding-right:8px; display: inline-block; ">
                <p><img style="border:0px; margin:0px; padding:0px;" alt="Resiste icon" src="data:image/png;base64," /></p>
            </div>
            <div style="vertical-align:top; border:0px solid black; display: inline-block; width:90%; font-family: 'Franklin Gothic Book',sans-serif;">
                <p>${projectFinalName}<br /><a href="${projectUrl}">${projectUrl}</a></p>
            </div>
            <div style="clear: both;"></div>
        </div>
        <!-- end version -->
    </body>
</html>

